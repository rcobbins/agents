{
  "name": "agent-framework",
  "version": "1.0.0",
  "description": "Multi-agent orchestration framework for automated software development",
  "main": "index.js",
  "scripts": {
    "setup": "./setup.sh",
    "start": "node web-ui/server/index.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cd web-ui/server && npm run dev",
    "dev:client": "cd web-ui/client && npm start",
    "build": "npm run build:client",
    "build:client": "cd web-ui/client && npm run build",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write .",
    "validate": "npm run lint && npm run test",
    "postinstall": "npm run install:server && npm run install:client",
    "install:server": "cd web-ui/server && npm install",
    "install:client": "cd web-ui/client && npm install"
  },
  "keywords": [
    "agents",
    "automation",
    "orchestration",
    "development",
    "ai",
    "framework"
  ],
  "author": "Framework Team",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "inquirer": "^9.2.0",
    "winston": "^3.11.0",
    "yargs": "^17.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "concurrently": "^8.2.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.2.0",
    "typescript": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/agent-framework"
  },
  "bugs": {
    "url": "https://github.com/your-org/agent-framework/issues"
  },
  "homepage": "https://github.com/your-org/agent-framework#readme",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "agents/**/*.js",
      "utils/**/*.js",
      "web-ui/server/**/*.js"
    ]
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "env": {
      "node": true,
      "es2021": true
    },
    "parserOptions": {
      "ecmaVersion": 2021
    },
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }
}